{
  "name": "@kainstar/vite-plugin-i18next-loader",
  "version": "1.0.2",
  "description": "Vite plugin loader for client embedded i18next locales composited from one to many json or yaml files.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "cm": "cz",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release",
    "test": "vitest --config vitest.config.ts run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kainstar/vite-plugin-i18next-loader.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kainstar",
    "email": "15249633+kainstar@users.noreply.github.com",
    "url": "https://github.com/kainstar"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "loader",
    "i18next",
    "locales",
    "yaml",
    "json"
  ],
  "bugs": {
    "url": "https://github.com/kainstar/vite-plugin-i18next-loader/issues"
  },
  "homepage": "https://github.com/kainstar/vite-plugin-i18next-loader#readme",
  "dependencies": {
    "dot-prop": "^8.0.0",
    "globby": "^14.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.0",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitest/coverage-v8": "^0.34.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.4",
    "semantic-release": "^22.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
